<!DOCTYPE html>
<html>
  <head>
    <title>Atomic</title>
    <%= stylesheet_link_tag    "application" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <ul class="hmenu">
      <%= render 'devise/menu/registration_items' %>
      <%= render 'devise/menu/login_items' %>
    </ul>
    <% flash.each do |key, msg| %>
      <%= content_tag :p, msg, :class => "flash #{key}" %>
    <% end %>

    <%= yield %>

  </body>
</html>
